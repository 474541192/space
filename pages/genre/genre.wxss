.box{width: 100%;height: 88rpx;background:#F9F9F9;border:1px solid white;}
.box-input{width:697rpx;height:65rpx;margin-left:26rpx;background: #fff;border-radius:5rpx;border:1px solid white;margin-top:10rpx;display: flex;align-items: center;font-size: 25rpx;color:#F9F9F9;}
.box-input image{height:40rpx;width: 40rpx;}
.box-list{width:160rpx;background:#F9F9F9;border:1px solid #F9F9F9;}
.box-list view{margin:0;padding: 0;width:165rpx;text-align: center;height:91rpx;line-height:80rpx;font-size:30rpx;color:#8B8B8B;}
.active{
    background: #fff;
    border-left:2px solid red;
    color:black;
    font-weight: bold;
  }
  .box-shopp{display: flex;}
  .box-nav{width:584rpx;height: 300rpx;border:1px solid white;}
  .box-nav .nav-all image{width:110rpx;height:114rpx;margin-top:54rpx;}
  .box-nav .nav-all text{font-size: 25rpx;margin-left:28rpx;}
  .box-nav .nav-all view:nth-of-type(1){margin-left: 47rpx;}
  .box-nav .nav-all view:nth-of-type(2){margin-top:26rpx;margin-left: 48rpx;font-size:28rpx;}
  .box-nav .nav-one{margin-top:44rpx;}
  .box-nav .nav-one text{font-size: 25rpx;margin-left:28rpx;}
  .box-nav .nav-one .big{display: flex;}
  .box-nav .nav-one .big .bao{margin-left:30rpx;font-size:28rpx;}
  .box-nav .nav-one .big .bao image{width:162rpx;height: 155rpx;}
  .font-eight{margin-left:30rpx;}
  .font-nine{margin-left:25rpx;}
  .box-nav .nav-two{margin-top:44rpx;width: 100%;}
  .box-nav .nav-two text{font-size: 25rpx;margin-left:28rpx;}
  .box-nav .nav-two .big{display: flex;justify-content: space-between;align-items: center;box-sizing: border-box;padding: 0 40rpx 0;}
  .box-nav .nav-two .big .bao{font-size:28rpx;}
  .box-nav .nav-two .big .bao image{width:132rpx;height: 130rpx;}
  .font-two{margin-left: 25rpx;} 
  .font-one{margin-right:5rpx;}
  .font-four{margin-left:24rpx;}
  .box-nav .nav-two .big .bao view:nth-last-of-type(1){margin-top:20rpx}
  .box-nav .nav-three{margin-left:40rpx;margin-top: 20rpx;}
  .box-nav .nav-three .big{display: flex;}
  .box-nav .nav-three .big .bao{font-size:28rpx;}
  .box-nav .nav-three .big .bao image{width:132rpx;height: 130rpx;}
  .font-five{margin-left:58rpx;}
  .font-seven{margin-left:25rpx;}
  .font-six{margin-left:60rpx;}
  .box-nav .nav-three .big .bao view:nth-last-of-type(1){margin-top:20rpx}